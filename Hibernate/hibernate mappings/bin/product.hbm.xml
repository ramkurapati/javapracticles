<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>

	<class name="roseindia.Product" table="product">
		<id name="id" type="int" column="id" >
			<generator class="increment"/>
		</id>
	  
		<property 
		  	name="qty"
			type="int"
			column="qty"
		/>
		
		<property 
		  	name="invoiceId"
			type="int"
			column="invoiceId"
		/>
		
		<many-to-one name="invoice" class="roseindia.Invoice" column="PRODUCT_ID"/>
		
	</class>

</hibernate-mapping>