<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "hibernate-mapping-3.0.dtd">

<hibernate-mapping>

 <class name="com.project.module.model.Meeting" table="meeting">
	 <id name="meetingId" type="string">
		   <column name="meeting_id"/>
  	 </id>
     <property name="MeetingName" column="meeting_name" type="string"/>
     <property name="MeetingTime" column="meeting_time" type="date"/>
    
    
 </class>
 
</hibernate-mapping>
